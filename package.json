{
  "name": "meal-management-system",
  "version": "1.0.0",
  "description": "A comprehensive meal management system for managing breakfast, lunch and dinner",
  "main": "server/index.ts",
  "scripts": {
    "start": "tsx server/index.ts",
    "server": "tsx watch server/index.ts",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "seed:superadmin": "tsx server/seedSuperAdmin.ts",
    "typecheck": "tsc --noEmit -p server/tsconfig.json"
  },
  "keywords": [
    "meal",
    "management",
    "mern",
    "mongodb",
    "express",
    "react",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "ua-parser-js": "^2.0.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.10",
    "@types/pdfkit": "^0.17.4",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/ua-parser-js": "^0.7.39",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
